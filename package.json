{
  "name": "cms-backend",
  "version": "0.2.0",
  "description": "CMS API testing branch",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV='production' node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "NODE_ENV='migration' node --require ts-node/register ./node_modules/typeorm/cli.js --config src/configs/ormconfig.ts",
    "prepare": "husky install",
    "repl": "ts-node repl/repl.ts"
  },
  "dependencies": {
    "@fleekhq/fleek-storage-js": "^1.0.20",
    "@metaio/auth-sdk": "^0.2.2",
    "@metaio/meta-signature-util": "^0.5.0",
    "@metaio/microservice-model": "^1.0.1",
    "@metaio/worker-model": "1.6.5",
    "@nestjs/axios": "^0.0.1",
    "@nestjs/bull": "^0.4.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "^8.0.6",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/swagger": "^5.0.8",
    "@nestjs/typeorm": "^8.0.1",
    "arweave": "^1.10.18",
    "bull": "^3.28.1",
    "cache-manager": "^3.4.4",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "dockerode": "3.3.1",
    "find-remove": "^3.1.0",
    "form-cors": "^1.0.2",
    "han": "^0.0.7",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "nats": "^2.1.0",
    "nest-winston": "^1.5.0",
    "nestjs-typeorm-paginate": "^2.6.0",
    "octokit": "^1.4.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "superagent": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.34",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bull": "^3.15.3",
    "@types/cache-manager": "^3.4.2",
    "@types/cache-manager-ioredis": "^2.0.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cron": "^1.7.3",
    "@types/dockerode": "^3.2.7",
    "@types/express": "^4.17.13",
    "@types/find-remove": "^2.0.1",
    "@types/jest": "^26.0.24",
    "@types/js-yaml": "^4.0.2",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": ">=6",
    "jest": "27.0.6",
    "js-yaml": "^4.1.0",
    "lint-staged": ">=10",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  },
  "engines": {
    "node": ">=14 <15"
  }
}
